{
  "name": "normalize-transforms",
  "description": "Transforms for normalize-walker",
  "version": "0.3.0",
  "author": {
    "name": "Jonathan Ong",
    "email": "me@jongleberry.com",
    "url": "http://jongleberry.com",
    "twitter": "https://twitter.com/jongleberry"
  },
  "license": "MIT",
  "repository": "normalize/transforms.js",
  "dependencies": {
    "debug": "*",
    "lazyrequire": "1",
    "composition": "1",
    "comment-shebang": "1",
    "esprima-error-formatter": "1",
    "normalize-log": "0",
    "normalize-walker": "0",

    "is-module": "1",
    "jstransform": "6",
    "es6-module-jstransform": "0",

    "recast": "0",
    "regenerator": "0",
    "es6-class": "0",
    "es6-arrow-function": "0",
    "es6-templates": "0",
    "es6-rest-params": "0",
    "es6-default-params": "0",
    "es6-spread": "0",
    "es6-comprehensions": "0",

    "autoprefixer": "2",
    "clean-css": "2",
    "uglify-js": "2"
  },
  "devDependencies": {
    "jade": "1",
    "marked": "0",
    "react-tools": "0",
    "coffee-script": "1",
    "less": "1",
    "node-sass": "0",
    "stylus": "0",

    "co": "3",
    "co-fs": "1",
    
    "mocha": "1",
    "should": "3",
    "istanbul-harmony": "0"
  },
  "main": "lib",
  "scripts": {
    "test": "mocha --harmony-generators --reporter spec --require should --bail",
    "test-cov": "node --harmony-generators node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --require should",
    "test-travis": "node --harmony-generators node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter dot --require should"
  },
  "files": ["lib"]
}
